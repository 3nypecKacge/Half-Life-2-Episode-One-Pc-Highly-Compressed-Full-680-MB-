{
  "name": "chameleon-ext",
  "description": "Spoof your browser profile. Includes a few privacy enhancing options.",
  "version": "v0.20.0",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
